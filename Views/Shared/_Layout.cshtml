<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="~/Content/Site.css">

</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top" dir="rtl">
        <div class="container-fluid">
            <a href="#" class="navbar-left"><img src="data:image/png;base64," width="60"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("تعريف المكتب", "Index", "Home")</li>
                    <li>@Html.ActionLink("تعديل بيانات قضية", "Index", "Home")</li>
                    <li>@Html.ActionLink("الاستعلام عن قضية", "Index", "Home")</li>

                    <li>@Html.ActionLink("للاتصال بنا", "Contact", "Home")</li>

                </ul>
            </div>
            <div class="collapse navbar-collapse navbar-light">
                <li>@Html.ActionLink("تسجيل الدخول", "Index", "Home")</li>
                <li>@Html.ActionLink("مستخدم جديد", "Contact", "Home")</li>
            </div>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />
        <hr />

        <footer>
            <center>
                <h6 style="font-size:10px;font-weight:400">&copy; @DateTime.Now.Year - مكتب المستشار للاستشارات القانونية</h6>
            </center>
        </footer>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>

</body>
</html>